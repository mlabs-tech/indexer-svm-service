{
  "name": "cryptarena-indexer",
  "version": "1.0.0",
  "description": "Solana indexer service for Cryptarena protocol",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "backfill": "ts-node scripts/backfill.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@prisma/client": "6.8.2",
    "@solana/web3.js": "^1.95.4",
    "bs58": "^6.0.0",
    "bullmq": "^5.21.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0"
  },
  "devDependencies": {
    "@types/bs58": "^5.0.0",
    "@types/node": "^22.9.0",
    "prisma": "6.8.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
